{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["src/css/*", "src/scss/*", "App.test.js" ],
    "rules": {
        "jsx-a11y/label-has-associated-control": [ 2, {
            "assert": "either"
          }]
    }
}
